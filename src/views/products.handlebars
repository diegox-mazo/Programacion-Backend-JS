<nav class="navbar navbar-expand-lg navbar-light bg-light">
    <div class="container-fluid">
        
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="/api/session/logout">LogOut</a>
                </li>
            </ul>

        </div>

    </div>
</nav>

<div>
    <h2> Bienvenido {{user.first_name}} {{user.last_name}}</h2>
    <h3>Rol {{user.rol}}</h3>
</div>

<div>
    <div class="d-flex align-content-center flex-wrap">
        {{#each products}}
        <div class="card border border-primary" style="width: 18rem;">
            {{!-- <img src="..." class="card-img-top" alt="..."> --}}
            <div class="card-body">
                <h3 class="card-text">{{this.title}}</h3>
                <p class="card-text">{{this.description}}</p>
                <p class="card-text">Code: {{this.code}}</p>
                <p class="card-text">Category: {{this.category}}</p>
                <p class="card-text">Stock: {{this.stock}}</p>
                <p class="card-text">Price: ${{this.price}}</p>
                <a class="btn btn-primary" href="#" role="button">Agregar al Carrito</a>
            </div>
        </div>
        {{/each}}
    </div>

    <div>
        {{#if hasPrevPage}}
        <div>Pagina Previa {{prevPage}}</div>
        <a href="/products?page={{prevPage}}"><button>Anterior</button></a>
        {{/if}}
    </div>

    <div>
        {{#if hasPrevPage}}
        <div>Pagina Siguiente {{nextPage}}</div>
        <a href="/products?page={{nextPage}}"><button>Siguiente</button></a>
        {{/if}}
    </div>


</div>